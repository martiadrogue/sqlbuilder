{
    "name": "martiadrogue/sqlbuilder",
    "type": "library",
    "description": "An electric and powerful SqlBuilder.",
    "keywords": [
        "martiadrogue",
        "sql",
        "sqlbuilder",
        "builder",
        "query"
    ],
    "homepage": "https://github.com/martiadrogue/sqlbuilder",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/martiadrogue/sqlbuilder/issues",
        "source": "https://github.com/martiadrogue/sqlbuilder"
    },
    "authors": [
        {
            "name": "Martí Adrogué",
            "email": "marti.adrogue@gmail.com",
            "homepage": "https://github.com/martiadrogue",
            "role": "Developer"
        },
        {
            "name": "sqlbuilder Contributors",
            "homepage": "https://github.com/martiadrogue/sqlbuilder/contributors"
        }
    ],
    "require": {
      "php": "~5.5|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "mockery/mockery": "~0.9",
        "squizlabs/php_codesniffer": "~2.3",
        "phpmd/phpmd": "~2.4"
    },
    "suggest": {
        "ext/xdebug": "Debugger and profiling"
    },
    "autoload": {
        "psr-4": {
            "MartiAdrogue\\SqlBuilder\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MartiAdrogue\\SqlBuilder\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "format": [
            "php-cs-fixer fix --level=psr2 src/",
            "phpcbf --standard=psr2 src/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "optimize-autoloader": true
    }
}
